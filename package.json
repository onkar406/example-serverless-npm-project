{
  "name": "example-serverless-npm-project",
  "version": "1.0.0",
  "description": "An example project to get started with Serverless, Node.js and npm",
  "main": "handler.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "local": "serverless offline",
    "deploy": "serverless deploy",
    "test": "mocha **/test/*",
    "test-jest": "jest --ci --coverage --color",
    "allure-generator": "allure generate",
    "cleanup": "rm -rf docs/jest-codecoverage/* docs/jest-unittest/*",
    "coverage": "nyc npm run test; nyc report --reporter=html --reporter=text-lcov --show-process-tree >coverage.lcov && codecov --disable=gcov"
  },
  "keywords": [
    "serverless",
    "npm"
  ],
  "author": "onkar406",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@jest-performance-reporter/core": "^2.1.2",
    "babel-jest": "^28.1.3",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "jest": "^28.1.3",
    "jest-allure": "^0.1.3",
    "jest-dashboard": "^0.3.1",
    "jest-github-reporter": "^1.0.2",
    "jest-html-reporter": "^3.6.0",
    "jest-html-reporters": "^3.0.10",
    "jest-junit": "^14.0.0",
    "jest-simple-dot-reporter": "^1.0.5",
    "jest-standard-reporter": "^2.0.0",
    "jest-stare": "^2.4.1",
    "junit": "^1.4.9",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "serverless-offline": "^9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onkar406/example-serverless-npm-project.git"
  },
  "bugs": {
    "url": "https://github.com/onkar406/example-serverless-npm-project/issues"
  },
  "homepage": "https://github.com/onkar406/example-serverless-npm-project#readme",
  "dependencies": {
    "mathjs": "^11.0.1"
  }
}
